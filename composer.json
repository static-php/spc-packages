{
    "name": "static-php/spc-packages",
    "version": "1.0.0",
    "description": "Build packages using static-php-cli",
    "type": "application",
    "homepage": "https://static-php.dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Marc Henderkes",
            "email": "rpms@henderkes.com"
        }
    ],
    "support": {
        "issues": "https://github.com/static-php/spc-packages/issues"
    },
    "config": {
        "platform": {
            "php": "8.3.0"
        },
        "sort-packages": true
    },
    "prefer-stable": true,
    "require": {
        "php": ">=8.3",
        "crazywhalecc/static-php-cli": "dev-fix/php85",
        "laravel/helpers": "^1.7",
        "ext-zlib": "*",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    }
}
